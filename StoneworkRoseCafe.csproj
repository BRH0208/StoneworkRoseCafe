<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\references\tModLoader.targets" />
  <PropertyGroup>
    <AssemblyName>StoneworkRoseCafe</AssemblyName>
    <TargetFramework>net45</TargetFramework>
    <PlatformTarget>x86</PlatformTarget>
    <LangVersion>7.3</LangVersion>
  </PropertyGroup>
  <Target Name="BuildMod" AfterTargets="Build">
    <Exec Command="&quot;$(tMLBuildServerPath)&quot; -build $(ProjectDir) -eac $(TargetPath) -define &quot;$(DefineConstants)&quot; -unsafe $(AllowUnsafeBlocks)" />
  </Target>
  <ItemGroup>
    <Compile Remove="Mugs\Items\GlassMugItem.cs" />
    <Compile Remove="Mugs\Items\GlazedStoneMugItem.cs" />
    <Compile Remove="Mugs\Items\HeatTreatedStoneMugItem.cs" />
    <Compile Remove="Mugs\Items\StoneMugItem.cs" />
    <Compile Remove="Mugs\Tiles\GlassMugTile.cs" />
    <Compile Remove="Mugs\Tiles\GlazedStoneMugTile.cs" />
    <Compile Remove="Mugs\Tiles\HeatTreatedStoneMugTile.cs" />
    <Compile Remove="Mugs\Tiles\StoneMugTile.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Mugs\Items\GlazedStoneMugItem.png" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="tModLoader.CodeAssist" Version="0.1.*" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Items\Mugs\" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="Microsoft.Build.Framework" />
  </ItemGroup>
</Project>